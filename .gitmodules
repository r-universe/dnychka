[submodule ".registry"]
	path = .registry
	url = https://github.com/r-universe-org/cran-to-git
	shallow = true
	branch = HEAD
[submodule "fields"]
	path = fields
	url = https://github.com/dnychka/fieldsrpackage
	shallow = true
	subdir = fields
	registered = true
[submodule "LatticeKrig"]
	path = LatticeKrig
	url = https://github.com/cran/LatticeKrig
	shallow = true
	registered = true
[submodule "RadioSonde"]
	path = RadioSonde
	url = https://github.com/cran/RadioSonde
	shallow = true
	registered = true
